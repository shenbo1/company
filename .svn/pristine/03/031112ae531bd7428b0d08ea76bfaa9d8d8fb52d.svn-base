    <script src="/Content/js/plugins/vue/vue.min.js"></script>
    <script src="/Content/js/plugins/layer/layer.min.js"></script>
    <script>
        var v = new Vue({
            el: "#main",
            data: {
                WorkModel: "",
                UserList:[]
            },
            created: function () {
                this.getWorkById();
                this.getUser();
            },
            methods: {
                getWorkById: function () {
                    Loading.Show();
                    var $this = this;
                    var url = "@Url.Action("WorkDetailById")";
                    var Data = {};
                    Data.Id = GetQueryString('id');
                    ajaxGet.post(Data, url, function (json) {
                        Loading.Hide();
                        if (json.IsSuccess) {
                            $this.WorkModel = json.Data;
                            //$this.UserList = json.Data.UserList;
                            Loading.Hide();
                            console.log(json.Data);
                        }
                        else {
                            Loading.Hide();
                        }
                    }, function (data) {
                        ExceptionDeal(data);
                    })
                },
                 getUser: function () {
                    var $this = this;
                    var url = "@Url.Action("CompanyUserListNotPager")";
                    var Data = {};
                    ajaxGet.post(Data, url, function (json) {
                        if (json.IsSuccess) {
                            $this.UserList = json.Data;
                        }
                        else {
                        }
                    }, function (data) {
                        ExceptionDeal(data);
                    })
                },
                //删除管理员
                delAdmin: function (event, item,Type) {
                    var $this = this;
                    layer.confirm('确定'+(Type==2 ? "添加":"删除")+'管理员[' + item.Name + ']?', {
                        btn: ['确定', '取消'] //按钮
                    }, function () {
                        var url = "@Url.Action("WorkDetailAdminEdit")";
                        var Data = {};
                        Data.WorkId = $this.WorkModel.Id;
                        Data.UserName = item.UserName;
                        Data.Type = Type;
                        ajaxGet.post(Data, url, function (data) {
                            if (data.IsSuccess) {
                                layer.msg(data.Message, { icon: 1 });
                                $this.getWorkById();
                                layer.closeAll();
                            }
                            else {
                                layer.msg(data.Message, { icon: 2 });
                            }
                            // Search();
                        }, function (data) { ExceptionDeal(data); })
                        // layer.msg('的确很重要', { icon: 1 });
                    }, function () {

                    });
                },
                ShowUserList: function () {
                    layer.open({
                        type: 1,
                        shade: false,
                        title: '添加团队成员', //不显示标题
                        content: $('#userList'), //捕获的元素，注意：最好该指定的元素要存放在body最外层，否则可能被其它的相对元素所影响
                        cancel: function () {
                        }
                    });
                },
                DoItems: function (event, item) {
                         var $this = this;
                     var url = "@Url.Action("WorkDetailStepEdit")";
                        var Data = {};
                         Data.ItemId = item.Id;
                        ajaxGet.post(Data, url, function (data) {
                            if (data.IsSuccess) {
                                layer.msg(data.Message, { icon: 1 });
                                $this.getWorkById();
                                //layer.closeAll();
                            }
                            else {
                                layer.msg(data.Message, { icon: 2 });
                            }
                            // Search();
                    }, function (data) { ExceptionDeal(data); })
                    //
                }
            },
            filters: {
                FormatDate: function (value) {
                    return ChangeDt(value);
                },
                Operate: function (item) {
                    var html = "";
                    switch (item) {
                        case "HasPei": html="开始处理"; break;
                        case "InHand": html= "完成"; break;
                        case "HasYu": html = "完成"; break;
                    }
                    return html;
                }
            }
        });
        $(document).ready(function () {
            $("#loading-example-btn").click(function () {
                btn = $(this); simpleLoad(btn, true);
                simpleLoad(btn, false)
            })
        });

        function simpleLoad(btn, state) {
            if (state) {
                btn.children().addClass("fa-spin");
                btn.contents().last().replaceWith(" Loading")
            } else {
                setTimeout(function () {
                    btn.children().removeClass("fa-spin"); btn.contents().last().replaceWith(" Refresh")
                }, 2000)
            }
        };
    </script>
</body>


